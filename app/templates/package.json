    "test:coverage": "scripty",
    "test:unit": "scripty",
    "alog": "tail -f logs/access.log.`date +%Y-%m-%d`",
    "plog": "tail -f logs/rock.error.log logs/rock.log",
    "flog": "tail -f logs/framework.log.`date +%Y-%m-%d`",
    "logs": "tail -f logs/*"
