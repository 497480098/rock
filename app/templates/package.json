    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags",
    "dev": "./node_modules/.bin/concurrently --kill-others -p name -c \"white.bgMagenta,white.bgBlue\" -n \"webpack,gulp   ,server \" \"npm run dev:asset\" \"npm run dev:gulp\" \"npm run dev:server\"",
    "dev:gulp": "gulp --color",
    "dev:server": "nodemon --watch server index.js",
    "dev:asset": "node ./webpack/webpack-dev-server.js",
    "build:release": "scripty",
